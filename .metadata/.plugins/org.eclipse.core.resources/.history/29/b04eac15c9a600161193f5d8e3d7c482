package Tanque;

public class Nivel1 extends Nivel {

	
	public Nivel1(){
		 simultaneo=1;
		 rutaGrafica="Jugador";
		 velocidadMov=2;
		 velocidadDisp=1;
		 aguante=1;
	}

	@Override
	public int getSimultaneo() {
		return simultaneo;
	}

	public String getRutaGrafica() {
		return rutaGrafica;
	}

	public float getVelocidadMov() {
		return velocidadMov;
	}

	public float getVelocidadDisp() {
		return velocidadDisp;
	}

	public int getVida() {
		return aguante;
	}

	public Nivel subirNivel() {
		return new Nivel2();
	}
	
	public Nivel bajarNivel(){
		return this;
	}
}
